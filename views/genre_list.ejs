<%- include('./partials/head') %>

<body>

  <%- include('./partials/header') %>
  
  <main>
    <h2><%= title %></h2>
    <% if (genre_list.length) { %>
        <ul>
            <% genre_list.forEach((genre) => { %>
                <li><a href="<%= genre.url %>"><%= genre.name %></a> 
                </li>
            <% }) %>
        </ul>
   <% } else { %>
    <p>There are no genres.</p>
    <% } %>
  </main>

</body>

<%- include('./partials/footer') %>